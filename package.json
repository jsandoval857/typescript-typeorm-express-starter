{
   "name": "typeorm-express-starter-kit",
   "version": "0.0.1",
   "description": "A TypeScript + TypeORM + Express Starter Kit",
   "main": "index.js",
   "scripts": {
      "build": "tsc",
      "dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' ./src/server.ts",
      "start": "ts-node src/index.ts",
      "prod": "npm run build && npm run start"
   },
   "author": "Omar Cruz <omar@lesi.app>",
   "license": "ISC",
   "dependencies": {
      "body-parser": "^1.19.0",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "lodash": "^4.17.15",
      "pg": "^7.3.0",
      "reflect-metadata": "^0.1.10",
      "typeorm": "0.2.20"
   },
   "devDependencies": {
      "nodemon": "^1.19.4",
      "@types/express": "^4.17.1",
      "ts-node": "3.3.0",
      "@types/node": "^8.0.29",
      "typescript": "3.3.3333"
   }
}
