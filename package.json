{
   "name": "typeorm-express-starter-kit",
   "version": "0.0.1",
   "description": "A TypeScript + TypeORM + Express Starter Kit",
   "main": "index.js",
   "scripts": {
      "build": "tsc",
      "dev": "set debug=* && ts-node-dev --respawn --transpileOnly ./src/server.ts",
      "start": "node ./build/server.js",
      "prod": "npm run build && npm run start",
      "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run"
   },
   "author": "Omar Cruz <omar@lesi.app>",
   "license": "ISC",
   "dependencies": {
      "bcryptjs": "^2.4.3",
      "body-parser": "^1.19.0",
      "class-validator": "^0.10.2",
      "cors": "^2.8.5",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "helmet": "^3.21.1",
      "jsonwebtoken": "^8.5.1",
      "lodash": "^4.17.15",
      "pg": "^7.12.1",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.2.20"
   },
   "devDependencies": {
      "@types/bcryptjs": "^2.4.2",
      "@types/body-parser": "^1.17.1",
      "@types/cors": "^2.8.6",
      "@types/express": "^4.17.1",
      "@types/helmet": "0.0.44",
      "@types/jsonwebtoken": "^8.3.5",
      "@types/node": "^12.11.1",
      "nodemon": "^1.19.4",
      "ts-node": "8.4.1",
      "ts-node-dev": "^1.0.0-pre.43",
      "typescript": "3.6.4"
   }
}